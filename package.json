{
  "name": "@vpmedia/bitcharify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/bitcharify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/bitcharify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/bitcharify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/bitcharify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "build": "vite build --emptyOutDir",
    "check": "pnpm lint && pnpm test && pnpm typecheck",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,css}\"",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "preview": "vite preview --open",
    "start": "vite --open",
    "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "test": "vitest --coverage --pass-with-no-tests",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@vpmedia/bitcharify": "^1.22.0",
    "@vpmedia/bitcharify-phaser2": "^1.11.0",
    "@vpmedia/bitcharify-phaser3": "^1.9.0",
    "@vpmedia/phaser": "^1.100.0",
    "phaser": "^3.90.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^62.5.0",
    "eslint-plugin-oxlint": "^1.42.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.2.0",
    "jsdom": "^27.4.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.4",
    "prettier": "^3.8.1",
    "stylelint": "^17.1.0",
    "stylelint-config-standard": "^40.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "8.0.0-beta.11",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "vite": "8.0.0-beta.11"
    }
  }
}
