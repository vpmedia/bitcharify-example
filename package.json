{
  "name": "@vpmedia/bitcharify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/bitcharify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/bitcharify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/bitcharify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/bitcharify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "typecheck": "tsc",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,md,css}\"",
    "test": "vitest --coverage --pass-with-no-tests"
  },
  "dependencies": {
    "@types/node": "^24.3.0",
    "@vpmedia/bitcharify": "^1.16.0",
    "@vpmedia/bitcharify-phaser2": "^1.7.0",
    "@vpmedia/bitcharify-phaser3": "^1.7.0",
    "@vpmedia/phaser": "^1.82.0",
    "phaser": "^3.90.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-jsdoc": "^54.3.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all",
    "iOS >= 14"
  ]
}
