{
  "name": "@vpmedia/bitcharify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/bitcharify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/bitcharify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/bitcharify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/bitcharify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "typecheck": "tsc",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,md,css}\"",
    "test": "vitest --coverage --pass-with-no-tests"
  },
  "dependencies": {
    "@vpmedia/bitcharify": "^1.18.0",
    "@vpmedia/bitcharify-phaser2": "^1.9.0",
    "@vpmedia/bitcharify-phaser3": "^1.8.0",
    "@vpmedia/phaser": "^1.85.0",
    "phaser": "^3.90.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-plugin-jsdoc": "^61.2.1",
    "eslint-plugin-oxlint": "^1.28.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "oxlint": "^1.28.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all",
    "iOS >= 14"
  ]
}
