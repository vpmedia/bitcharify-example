{
  "name": "@vpmedia/bitcharify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/bitcharify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/bitcharify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/bitcharify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/bitcharify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "typecheck": "tsc",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,md,css}\"",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "@vpmedia/bitcharify": "^1.11.0",
    "@vpmedia/bitcharify-phaser2": "^1.6.0",
    "@vpmedia/bitcharify-phaser3": "^1.6.0",
    "@vpmedia/phaser": "^1.43.0",
    "phaser": "^3.85.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "eslint": "^9.11.1",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.7.16",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.7"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all"
  ]
}
