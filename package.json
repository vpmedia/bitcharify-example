{
  "name": "@vpmedia/bitcharify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/bitcharify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/bitcharify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/bitcharify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/bitcharify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{css,js,jsx}\"",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "@vpmedia/bitcharify": "^1.11.0",
    "@vpmedia/bitcharify-phaser2": "^1.6.0",
    "@vpmedia/bitcharify-phaser3": "^1.6.0",
    "@vpmedia/phaser": "^1.43.0",
    "phaser": "^3.70.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsdoc": "^48.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.6.1",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  }
}
